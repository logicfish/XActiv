import org::eclipse::xtext;
import org::eclipse::xtext::generator;

extension org::eclipse::xtext::generator::Naming; 
extension org::eclipse::xtext::GrammarUtil;

String qualifiedInjectorProviderName(Grammar grammar) :
	qualifiedInjectorProviderName(grammar,getNaming());
	
String qualifiedInjectorProviderName(Grammar grammar, Naming n) :
	JAVA com.logicfishsoftware.xtend.xactive.generator.XActiveGeneratorFragment.getQualifiedInjectorProviderName(org.eclipse.xtext.Grammar,org.eclipse.xtext.generator.Naming);

String qualifiedUiInjectorProviderName(Grammar grammar) :
	qualifiedUiInjectorProviderName(grammar,getNaming());
	
String qualifiedUiInjectorProviderName(Grammar grammar, Naming n) :
	JAVA com.logicfishsoftware.xtend.xactive.generator.XActiveGeneratorFragment.getQualifiedUiInjectorProviderName(org.eclipse.xtext.Grammar,org.eclipse.xtext.generator.Naming);
